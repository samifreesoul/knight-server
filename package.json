    "finnhub": "^1.2.19",
    "metaapi.cloud-sdk": "^29.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.3",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-node": "^3.1.0"
  }
