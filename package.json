{
  "name": "knight-server",
  "version": "1.0.0",
  "description": "Node.js and Express server with TypeScript and Vite",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc && vite build",
    "start": "node dist/server.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
